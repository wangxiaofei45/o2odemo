/**app.wxss**/
@import "/wxParse/wxParse.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{
  background-color: #f5f5f5;
   height: 100%; 
}
.clearfix::after{
  display: block;
  clear: both;
  height: 0;
  content: 0;
  overflow: hidden;
}
.flex_center{
  display: flex;
  display: -webkit-flex;
  justify-content: center;
  align-items: center;
}
.flex_center_wrap{
  display: flex;
  display: -webkit-flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: center;
}
.flex_around{
  display: flex;
  display: -webkit-flex;
  justify-content: space-around;
  align-items: center;
}
.flex_around_wrap{
  display: flex;
  display: -webkit-flex;
  flex-wrap: wrap;
  justify-content: space-around;
  align-items: center;
}
.flex_between{
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  align-items: center;
}
.flex_between_wrap{
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
}
.flex_start{
  display: flex;
  display: -webkit-flex;
  justify-content: flex-start;
  align-items: center;
}
.flex_end{
  display: flex;
  display: -webkit-flex;
  justify-content: flex-end;
  align-items: center;
}
.flex_start_wrap{
  display: flex;
  display: -webkit-flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: flex-start;
}

.border_bottom{
  border-bottom: 1rpx solid #d6d6d6;
}
/*首页列表 首页搜索的列表显示*/
.show_con{
  width: 100%;
  margin-top: 22rpx; 
}
.show_list{
  width: 100%;
  height: 260rpx;
  border-top: 3rpx solid #e0e0e0;
  background-color: #ffffff;
}
.list_con{
  width: 700rpx;
  height: 200rpx;
  margin: 0 auto;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  box-sizing: content-box;
}

.list_con image{
  float: left;
  width: 200rpx;
  height: 200rpx;
  margin-right: 20rpx;
}
.list_con .right{
  width: 480rpx;
  height: 200rpx;
  float: left;
}
.title_con{
  height: 60rpx;
}
.title_con .title{
  float: left;
  font-size: 32rpx;
  color: #333333;
  font-weight: 700;
}
.title_con .nav{
  float: right;
  color: #666666;
}
.nav image{
  width: 60rpx;
  height: 60rpx;
  font-size: 20rpx;
}
.right .text1{
  color: #666666;
  font-size: 27rpx;
}
.right .text2{
  color: #666666;
  font-size: 24rpx;
  margin-top: 15rpx;
  height: 28rpx;
  line-height: 28rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.right .text3{
  color: #ff4965;
  font-size: 24rpx;
  margin-top: 24rpx;
}
.right button{
  height: 40rpx;
  font-size: 24rpx;
  line-height: 40rpx;
  padding: 0rpx 5rpx;
  color: #ffffff;
  background-color: #ff4965;
  float: left;
  margin-top: 10rpx;
  margin-right: 12rpx;
  border-radius: 8rpx;
  box-sizing: border-box;
}
/*收款码付款码*/
.collec_code_image{
  display: block;
  width: 86rpx;
  height: 86rpx;
  border-radius: 10rpx;
  margin: 0 auto;
  margin-top: 90rpx;
  margin-bottom: 38rpx;
}
.collec_code_name{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  color: #393a3f;
  font-size: 32rpx;
  text-align: center;
  letter-spacing: 3rpx;
  margin-bottom: 50rpx;
}
.collec_code_receivable{
  width: 565rpx;
  height: 30rpx;
  line-height: 30rpx;
  color: #393a3f;
  letter-spacing: 3rpx;
  margin: 0 auto;
  margin-bottom: 30rpx;
}
.collec_code_input{
  width: 565rpx;
  height: 120rpx;
  margin: 0 auto;
  margin-bottom: 50rpx;
  border-bottom: 3rpx solid #e0e0e0;
  position: relative;
  
}
.collec_code_input input{
  height: 110rpx;
  font-size: 105rpx;
  padding-left: 50rpx;
}
.collec_code_input image{
  width: 45rpx;
  height: 45rpx;
  position: absolute;
  left: 0rpx;
  top: 30rpx;
}
.collec_code_sure{
   width: 565rpx;
  height: 100rpx;
  margin: 0 auto;
  margin-top: 108rpx;
  background-color: #ff4965;
  color: #ffffff;
  font-size: 36rpx;
  font-weight: 700;
  letter-spacing: 5rpx;
}
.collec_code_sure:active{
  background-color: #ffffff;
}
.active{
  background-color: rgba(0, 0, 0, 0.2);
}
.view_hide{
    display: none;
}
.view_show{
    display: block;
}
.defaultpage{
  width: 100%;
}
.defaultpage image{
    width: 300rpx;
    height: 300rpx;
    display: block;
    margin: 0 auto;
    margin-top: 300rpx;
}
.defaultpage .text{
  width: 100%;
  font-size: 32rpx;
  color: #999999;
  text-align: center;
  letter-spacing: 5rpx;
}
.defaultpage button{
  margin-top: 70rpx;
  width: 280rpx;
  height: 90rpx;
  color: #666666;
  font-size: 32rpx;
  background-color: #ffffff;
  border-radius: 10rpx;
  border: 3rpx solid #b3b3b3;
  letter-spacing: 5rpx;
} 
/*添加切换身份显示*/
.shop{
  margin-top: 102rpx;
}
.select_identity {
  width: 100%;
  height: 90rpx;
  border-bottom: 1rpx solid #d6d6d6;
  background-color: #fff;
  font-size: 30rpx;
  position: relative;
}
.select_pos_abs{
  /* position: absolute;
  top: 92rpx;
  left: 0rpx;
  width: 100%;
  background-color: #ffffff; */
  width: 750rpx;
  height: 1760rpx;
  background-color: rgba(0, 0, 0, 0.3);
  position: absolute;
  left: 0rpx;
  top: 92rpx;
}
.select_pos_abs .list{
  line-height: 90rpx;
  padding-left: 69rpx;
  border-bottom: 1rpx solid #d6d6d6;
  background-color: #ffffff;
}
.select_identity .left {
  width: 50%;
  padding-left: 25rpx;
}
.select_identity .right {
  width: 50%;
  padding-right: 25rpx;
}
.select_identity image {
  width: 44rpx;
  height: 44rpx;
  padding: 10rpx;
}
.select_identity .name {
  color: #ff4965;
}
.select_identity .rotate{
  transform: rotate(180deg);
}