.containers{
  width: 700rpx;
  margin: 0 auto;
  margin-bottom: 50rpx;
}
.title{
  width: 100%;
  height: 130rpx;
  color: #333;
  font-size: 30rpx;
  font-weight: 600;
}
.title image{
  width: 80rpx;
  height: 80rpx;
}
.title .line{
  width: 460rpx;
  height: 3rpx;
  background-color: #333;
}
.listcon{
  width: 700rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
  border-left: 3rpx solid #d6d6d6;
  border-top: 3rpx solid #d6d6d6;
}
.listcon .list{
  font-size: 25rpx;
  box-sizing: border-box;
  width: 175rpx;
  height: 85rpx;
  line-height: 85rpx;
  color: #666666;
  text-align: center;
  background-color: #fff;
  border-right: 3rpx solid #d6d6d6;
  border-bottom: 3rpx solid #d6d6d6;
}