  background-color: #fff;
  margin-bottom: 22rpx;
}
.header .logo {
  width: 85rpx;
  height: 85rpx;
  display: block;
  margin: 0 auto;
  padding-top: 70rpx;
  padding-bottom: 40rpx;
  border-radius: 10rpx;
}

.header .name {
  width: 100%;
  text-align: center;
  color: #393a3f;
  font-size: 32rpx;
  padding-bottom: 80rpx;
}
.select_identity .nameg{
  color: #666666;
}
.hb_list {
  width: 200rpx;
  text-align: center;
  font-size: 24rpx;
  color: #333;
}

.hb_list image {
  padding-top: 10rpx;
  width: 60rpx;
  height: 60rpx;
  padding-bottom: 15rpx;
}

.hb_list .red {
  padding-bottom: 20rpx;
  color: #ff4965;
}
