.containars{
  width: 700rpx;
  margin: 0 auto;
}
.canvas_title{
  width: 100%;
  height: 125rpx;
  line-height: 125rpx;
  color: #333333;
  font-size: 32rpx;
   font-weight: 600; 
  letter-spacing: 9rpx;
  text-align: center;
}
.canvas{
  width: 160px;
  height: 160px;
  margin: 0 auto;
  margin-bottom: 60rpx;
}
.waring{
  font-size: 28rpx;
  color: #333333;
  font-weight: 700;
  padding-bottom: 30rpx;
  letter-spacing: 5rpx;
}
.input{
  width: 100%;
  height: 108rpx;
  border: 3rpx solid #d6d6d6;
  border-radius: 10rpx;
  box-sizing: border-box;
  position: relative;
  margin-bottom: 35rpx;
}
.input input{
  width: 100%;
  height: 100%;
  line-height: 100%;
  padding-left: 25rpx;
  font-size: 36rpx;
}
.posab{
  width: 50rpx;
  height: 50rpx;
  font-size: 40rpx;
  line-height: 50rpx;
  text-align: center;
  position: absolute;
  top: 29rpx;
  right: 10rpx;
}
.warings .waring_title{
  font-size: 22rpx;
  color: #333333;
  font-weight: 700;
  margin-bottom: 15rpx;
}
.warings .text{
  color: #999999;
  font-size: 22rpx;
  line-height: 36rpx;
}
.button{
  width: 700rpx;
  height: 90rpx;
  position: absolute;
  left: 25rpx;
  bottom: 50rpx;
}
.button button{
  width: 340rpx;
  height: 90rpx;
}
.button .submit{
  color:#ff4965;
}