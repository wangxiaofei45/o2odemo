page{
  background-color: #fff;
}
.containers{
  width: 700rpx;
  margin: 0 auto;
}

.containers .list{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 30rpx;
  border-bottom: 3rpx solid #d6d6d6;
}
.containers .list .name{
  color: #666666;
  width: 245rpx;
}
.containers .list image{
  width: 44rpx;
  height: 44rpx;
}
/*修改提示*/
.containers .list_upimg{
  width: 100%;
  height: 140rpx;
  font-size: 30rpx;
  line-height: 45rpx;
  color: #666666;
}
.containers .list_upimg .title_1{
  padding-top: 30rpx;
  padding-bottom: 10rpx;
}
.containers .list_upimg image{
  width: 44rpx;
  height: 44rpx;
}
.containers .list_upimg .color{
  color: #999999;
  font-size: 24rpx;
}
.containers .list .center{
   color: #333333;
   width: 405rpx;
   font-weight: 600;
}
.containers .list .grycenter{
   color: #666666;
}
.containers .header{
  font-size: 30rpx;
  color: #333;
  font-weight: 700;
}
.containers .listnone{
  color: #666666;
  border: none;
}
.containers .listnone .color{
  color: #999999;
  font-size: 24rpx;
}
.containers .feature{
   width: 100%;
   height: 70rpx;
   line-height: 70rpx;
   border: 3rpx solid #d6d6d6;
   border-radius: 10rpx;
}
.containers .introduce{
  width:100%;
  height: 200rpx;
  border: 3rpx solid #d6d6d6;
  border-radius: 10rpx;
}
.containers .picture{
  width: 182rpx;
  height: 182rpx;
  border: 3rpx dashed #d6d6d6;
  border-radius: 10rpx;
}
.containers .picture image{
  width: 180rpx!important;
  height: 180rpx!important;
}
.containers .picture .up_img{
   width: 90rpx!important;
  height: 90rpx!important;
}
.instore{
  width: 100%;
  height: 100%;
}
.instore .up_img{
  width: 90rpx;
  height: 90rpx;
}
.sure{
  width: 700rpx;
  background-color: #ff4965;
  color: #fff;
  margin-top: 75rpx;
  margin-bottom: 75rpx;
}
.active{
  background-color:rgba(0, 0, 0, 0.1)
}
.demo_img{
  width: 700rpx;
  height: 350rpx;
  margin: 0 auto;
  border: 1rpx dashed #d6d6d6;
  border-radius: 10rpx;
  overflow: hidden;
  position: relative;
}
.demo_img image{
  width: 700rpx;
  height: 350rpx;
}
.demo_img #up_img{
  width: 90rpx!important;
  height: 90rpx!important;
  margin-left: 305rpx;
  margin-top: 130rpx;
}