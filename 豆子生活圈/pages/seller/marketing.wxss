page {
  background-color: #f5f5f5;
}

.content {
  background-color: #fff;
  margin-bottom: 15rpx;
}

.header {
  width: 700rpx;
  height: 100rpx;
  font-size: 34rpx;
  margin: 0 auto;
  border-bottom: 1rpx solid #d6d6d6;
}
.header .right{
  font-size: 30rpx;
  color: #999999
}
.header .text {
  padding: 0rpx 20rpx;
}

.header image {
  width: 18rpx;
  height: 23rpx;
  padding-top: 10rpx;
}

.bar {
  width: 700rpx;
  margin: 0 auto;
}

.bar .list {
  width: 700rpx;
  font-size: 28rpx;
}

.bar .list .lists_con {
  width: 160rpx;
  text-align: center;
}

.bar .list image {
  width: 61rpx;
  height: 53rpx;
  margin-top: 30rpx;
  margin-bottom: 20rpx;
}
.bar .list .text {
  margin-bottom: 30rpx;
}
.bar_pos {
  width: 700rpx;
  height: 144rpx;
  margin: 0 auto;
  position: relative;
  overflow: hidden;
}
.bar_pos .list {
  width: 700rpx;
  height: 72rpx;
  font-size: 26rpx;
  color: #666666;
  line-height: 72rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.question{
  width: 700rpx;
  height: 70rpx;
  margin: 0 auto;
  border-bottom: 1rpx solid #d6d6d6;
  color: #575757;
  font-size: 24rpx;
}
.question image{
  width: 36rpx;
  height: 32rpx;
}
