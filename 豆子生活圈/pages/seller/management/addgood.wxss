page{
  background-color: #fff;
}
.containers{
  width: 700rpx;
  margin: 0 auto;
}
.containers .list{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 30rpx;
  border-bottom: 3rpx solid #d6d6d6;
}
.containers .list .name{
  color: #666666;
  width: 245rpx;
}
.containers .list image{
  width: 44rpx;
  height: 44rpx;
}
.containers .list .center{
   color: #333333;
   width: 405rpx;
   font-weight: 600;
}
.containers .list .grycenter{
   color: #666666;
}
.containers .header{
  font-size: 30rpx;
  color: #333;
  font-weight: 700;
}
.containers .listnone{
  color: #666666;
  border: none;
}
.containers .listnone .color{
  color: #999999;
  font-size: 24rpx;
}
.containers .feature{
   width: 100%;
   height: 70rpx;
   line-height: 70rpx;
   border: 3rpx solid #d6d6d6;
   border-radius: 10rpx;
}
.containers .introduce{
  width:100%;
  height: 200rpx;
  border: 3rpx solid #d6d6d6;
  border-radius: 10rpx;
}
.containers .picture{
  width: 182rpx;
  height: 182rpx;
  border: 3rpx dashed #d6d6d6;
  border-radius: 10rpx;
}
.containers .picture image{
  width: 180rpx!important;
  height: 180rpx!important;
}
.containers .picture .up_img{
  width: 90rpx!important;
  height: 90rpx!important;
}
.sure{
  width: 700rpx;
  background-color: #ff4965;
  color: #fff;
  margin-top: 75rpx;
  margin-bottom: 75rpx;
}
.active{
  background-color:rgba(0, 0, 0, 0.1)
}
button{
  border-radius: 0rpx;
}
.bottom{
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
  height: 90rpx;
  width: 750rpx;
  font-size: 32rpx;
  font-weight: 700;
  line-height: 90rpx;
  border-top: 1rpx solid #d6d6d6;
  box-sizing: border-box;
}
.bottom .left{
  width: 50%;
  color: #666666;
  height: 90rpx;
  text-align: center;
  background-color: #fff;
}
.bottom .right{
  width: 50%;
  color: #ffffff;
  background-color: #ff4965;
  text-align: center;
  height: 90rpx;
}