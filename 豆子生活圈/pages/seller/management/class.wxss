page{
  background-color: #ffffff;
}
.list{
  width: 750rpx;
  height: 90rpx;
  border-bottom: 1rpx solid #d6d6d6;
}
.list_cons{
  width: 700rpx;
  margin: 0 auto;
}
.list .left{
  font-size: 32rpx;
  color: #333333;
}
.list .left .gray{
  font-size: 24rpx;
  color: #999999;
}
.list image{
  width: 44rpx;
  height: 44rpx;
}
.add{
  position: fixed;
  right: 25rpx;
  bottom: 80rpx;
  width: 88rpx;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  border: 1rpx solid red;
  box-sizing: border-box;
  border-radius: 50%;
  box-shadow: 0 0 10rpx rgba(250,0,0,0.5);
  color: #b3b3b3;
  font-size: 80rpx;
}
/* 弹出框 */
.model{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
}
.model  .bottom_black{
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.4);
}
.model .modal_con{
  width: 540rpx;
  height: 410rpx;
  background-color: #ffffff;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-top: -205rpx;
  margin-left: -270rpx;
  border-radius: 10rpx;
}
.model .modal_con .title{
  color: #333333;
  font-size: 32rpx;
  text-align: center;
  margin-top: 50rpx;
  margin-bottom: 60rpx;
}
.model .modal_con .input{
  width: 400rpx;
  height: 80rpx;
  margin: 0 auto;
  margin-bottom: 65rpx;
  border-bottom: 1rpx solid #d6d6d6;
}
.model .modal_con .input input{
  width: 400rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 32rpx;
}
.model button{
  width: 400rpx;
  height: 90rpx;
  line-height: 90rpx;
  color: #ffffff;
  background-color:#ff4965;  
}