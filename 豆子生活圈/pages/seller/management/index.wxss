.content{
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.header{
  width: 750rpx;
  background-color: #fff;
  padding-top: 15rpx;
}
.header .seach{
  width: 700rpx;
  height: 60rpx;
  margin: 0 auto;
  box-sizing: border-box;
  border: 1rpx solid #d6d6d6;
  border-radius: 30rpx;
  overflow: hidden;
}
.header .seach image{
  width: 44rpx;
  height: 44rpx;
}
.header .seach input{
  width: 540rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 24rpx;
}
.header .seach .end{
  color: #fff;
  background-color: #ff4965;
  font-size: 30rpx;
  height: 60rpx;
  line-height: 60rpx;
  width: 100rpx;
  text-align: center;
}
.header .list{
  width: 700rpx;
  margin: 0 auto;
  margin-top: 50rpx;
  padding-bottom: 50rpx;
  font-size: 30rpx;
  color: #666666;
  font-weight: 700;
}
.header .list .list_cons{
  padding: 15rpx;
  text-align: center;
}
.header .list .list_cons image{
  width: 66rpx;
  height: 66rpx;
  margin-bottom: 10rpx;
}
/*过滤*/
.filter{
  width: 750rpx;
  height: 80rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #d6d6d6;
  font-size: 30rpx;
  margin-top: 15rpx;
}
.filter image{
  width: 28rpx;
  height: 28rpx;
  margin-left: 10rpx;
}
.filter .active{
  transform: rotate(180deg); 
}
.filter .left{
  width: 40%;
  height: 32rpx;
  line-height: 32rpx;
  border-right: 1rpx solid #d6d6d6;
  position: relative;
}
.filter .left .left_pos{
  width: 750rpx;
  height: 1760rpx;
  background-color: rgba(0, 0, 0, 0.3);
  position: absolute;
  left: 0rpx;
  top: 57rpx;
}
.filter .left .left_pos image{
  width: 44rpx;
  height: 44rpx;
}
.filter .left .left_pos .list{
  width: 750rpx;
  height: 80rpx;
  padding: 0rpx 25rpx;
  box-sizing: border-box;
  line-height: 80rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #d6d6d6;
}
.filter .center{
  width: 40%;
  height: 32rpx;
  line-height: 32rpx;
  border-right: 1rpx solid #d6d6d6;
  position: relative;
}
.filter .center .left_pos{
  width: 750rpx;
  height: 1760rpx;
  background-color: rgba(0, 0, 0, 0.3);
  position: absolute;
  left: -100%;
  top: 57rpx;
}
.filter .center .left_pos image{
  width: 44rpx;
  height: 44rpx;
}
.filter .center .left_pos .list{
  width: 750rpx;
  height: 80rpx;
  padding: 0rpx 25rpx;
  box-sizing: border-box;
  background-color: #fff;
  line-height: 80rpx;
  border-bottom: 1rpx solid #d6d6d6;
}
.filter .right{
  width: 20%;
  height: 32rpx;
  line-height: 32rpx;
  padding: 24rpx 0rpx;
}
.filterright{
  width: 100%;
  height: 100%;
  background-color: #ffffff;
  z-index: 100;
  position: fixed;
  left: 0rpx;
  right: 0rpx;
  top: 0rpx;
}
.filterright .title{
  width: 700rpx;
  height: 36rpx;
  line-height: 36rpx;
  margin: 0 auto;
  font-size: 32rpx;
  color: #333333;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}
.filterright .title .left{
  width: 10rpx;
  height: 36rpx;
  background-color: #ffcb65;
  border-radius: 5rpx;
  margin-right: 15rpx;
}
.filterright .list_cons{
  width: 700rpx;
  margin: 0 auto;
}
.filterright .list_cons .list{
  width: 142rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 32rpx;
  color: #333333;
  text-align: center;
  background-color: #f5f5f5;
  border: 1rpx solid #ebebeb;
  box-sizing: border-box;
  border-radius: 5rpx;
  margin-right: 44rpx;
  margin-bottom: 35rpx;
}
.filterright .list_cons .active{
  background-color: #fae5e3;
  border: 1rpx solid #ff4965;
}
.filterright .list_cons .list:nth-child(4n){
  margin-right: 0rpx;
}
.filterright .bottom{
  width: 750rpx;
  height: 90rpx;
  position: absolute;
  left: 0rpx;
  bottom: 0rpx;
  border-top: 1rpx solid #d6d6d6;
  box-sizing: border-box;
}
.filterright .bottom .left{
  width: 50%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
}
.filterright .bottom .right{
  width: 50%;
  height: 90rpx;
  line-height: 90rpx;
   text-align: center;
  background-color: #ff4965;
  color: #ffffff;
}
/*下侧列表*/
.onlinechoose{
  width: 750rpx;
  background-color: #ffffff;
  font-size: 32rpx;
}
.onlinechoose .title{
  width: 750rpx;
  height: 90rpx;
  border-top: 1rpx solid #d6d6d6;
  border-bottom: 1rpx solid #d6d6d6;
}
.onlinechoose .title .change{
  width: 140rpx;
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
  box-sizing: border-box;
}
.onlinechoose .title .actives{
  border-bottom: 3rpx solid #ff4965;
  color: #ff4965;
}
.onlinechoose .hotel_list{
  width: 750rpx;
  height: 260rpx;
  border-bottom: 1rpx solid #d6d6d6;
  background-color: #ffffff;
}
.onlinechoose .hotel_list .list_cons{
  width: 700rpx;
  height: 200rpx;
}
.onlinechoose .list_cons image{
  width: 200rpx;
  height: 200rpx;
  margin-right: 25rpx;
}
.list_cons .right{
  width: 475rpx;
  height: 200rpx;
}
.list_cons .right .name{
  font-size: 32rpx;
  color: #333333;
  font-weight: 700;
  letter-spacing: 3rpx;
  margin-bottom: 20rpx;
}
.list_cons .right .list1{
    margin-bottom: 17rpx;
}
.list_cons .right .list1 .condition1{
  font-size: 28rpx;
  color: #999999;
  margin-bottom: 15rpx;
}
.list_cons .right .list1 .condition2{
  font-size: 24rpx;
  color: #999999;
}
.list_cons .right .list1 .act{
  color: #2d8622;
}
.list_cons .right .list1 .r{
  font-size: 24rpx;
  color: #ff4965;
}
.list_cons .right .list1 .big{
  font-size: 32rpx;
}
.list_cons .right .lists2{
  height: 36rpx;
   line-height: 36rpx;
   font-size: 22rpx;
}
.list_cons .right .lists2 .left{
  width: 162rpx;
  text-align: center; 
  color: #ff4965;
  border: 1rpx solid #ff4965;
  box-sizing: border-box;
  border-radius: 5rpx;
}
.list_cons .right .lists2 .right2{
  color: #999999;
}