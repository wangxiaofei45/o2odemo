.header{
  width: 100%;
  height: 200rpx;
  background-color: #fff;
  margin-bottom: 12rpx;
}
.header .left{
  border-right: 1rpx dashed #d6d6d6;
}
.header .list{
  width: 50%;
  text-align: center;
}
.header .name{
  font-size: 24rpx;
  color: #333;
}
.header .number{
  font-size: 32rpx;
  color: #FC7E92;
  padding-top: 36rpx;
}
.list_cons{
  width: 750rpx;
  background-color: #fff;
  padding: 0rpx 25rpx;
  box-sizing: border-box;
}
.list_cons .list{
  height: 140rpx;
}
.list image{
  width: 84rpx;
  height: 84rpx;
}