page{
  background-color: #f5f5f5;
}

.list{
  background-color: #ffffff;
  width: 100%;
  height: 90rpx;
  border-bottom: 3rpx solid #d6d6d6;
}
.pages{
  height: 110rpx;
}
.pages .right .user_img{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}
.list .right .go_img{
  width: 44rpx;
  height: 44rpx;
}
.list .left{
  font-size: 30rpx;
  color: #333333;
  padding-left: 25rpx;
}
.list .right{
  font-size: 30rpx;
  color: #999999;
  padding-right: 25rpx;
}
.list .right input{
  text-align: right;
  color: #333333;
}
.code .right .code{
  width: 60rpx;
  height: 60rpx;
}
/*model*/
.sex_model{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0rpx;
  top: 0rpx;
  background-color: rgba(0,0,0,0.4);
  z-index: 20;
}
.model_con{
  width: 650rpx;
  background-color: #ffffff;
  border-radius: 10rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-top: -150rpx;
  margin-left: -325rpx;
  overflow: hidden;
}
.model_con .sex_list{
   width:100%;
   height: 100rpx;
   line-height: 100rpx;
   padding-left: 25rpx; 
   border-bottom: 3rpx solid #d6d6d6;
}
.button{
  width: 750rpx;
  position: absolute;
  left: 0rpx;
  bottom: 0rpx;
}
.button button{
  width: 375rpx;
  height: 100rpx;
  border-radius: 0rpx;        
}
.button button::after{
  border: none;
  
}
.button button:nth-child(1){
  border-top: 3rpx solid #d6d6d6;
  border-right: 3rpx solid #d6d6d6;
}
.button button:nth-child(2){
  border-top: 3rpx solid #d6d6d6;
}
 .active{
  background-color: rgba(0,0,0,0.1);
} 