page{
  background-color: #ff4965;
}
.content{
  width: 700rpx;
  background-color: #fff;
  margin: 0 auto;
  border-radius: 18rpx;
  box-sizing: border-box;
  position: relative;
  padding-top: 40rpx;
}
.no_1{
  text-align: center;
}
.all_money{
  width: 330rpx;
  height: 118rpx;
  margin: 0 auto;
  margin-bottom: 60rpx;
  position: relative;
  color: #FEFF8B;
}
.all_money image{
  width: 330rpx;
  height: 118rpx;
}
.all_money .big{
  position: absolute;
  right:105rpx;
  top: 50%;
  margin-top: -18rpx;
  font-size: 32rpx;
}
.all_money .little{
  position: absolute;
  right: 24rpx;
  top: 50%;
  margin-top: -12rpx;
  font-size: 24rpx;
}
.name{
  font-size: 30rpx;
  color: #333;
  font-weight: 700;
  margin-bottom: 15rpx;
}
.money{
  font-size: 56rpx;
  color: #ff4965;
  font-weight: 700;
  margin-bottom: 22rpx;
}
.time{
  color: #666666;
  font-size: 24rpx;
  margin-bottom: 15rpx;
}
.center{
  width: 100%;
  height: 40rpx;
}
.lists_con{
  width: 620rpx;
  margin: 0 auto;
  color: #333333;
  font-size: 28rpx;
  margin-bottom: 50rpx;
}
.lists_con .list{
  margin-top: 10rpx;
  letter-spacing: 2rpx;
}
.lists_con image{
  width: 10rpx;
  height: 10rpx;
  margin-right: 15rpx;
}
.goto_detail{
  width: 620rpx;
  height: 80rpx;
  margin: 0 auto;
  font-size: 32rpx;
  color: #333333;
  font-weight: 700;
  padding: 15rpx 0rpx;
  border-top: 1rpx solid #D6D6D6;
}
.goto_detail image{
  width: 40rpx;
  height: 40rpx;
}