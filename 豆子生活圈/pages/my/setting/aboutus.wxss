page{
  background-color: #fff;
}
.header{
  width: 100%;
  height: 380rpx;
  border-bottom: 3rpx solid #d6d6d6;
}
image{
  width: 200rpx;
  height: 200rpx;
  display: block;
  margin: 0 auto;
  margin-top: 60rpx;
  margin-bottom: 30rpx;
}
.name{
  width: 100%;
  color: #333333;
  font-size: 30rpx;
  text-align: center;
  font-weight: 600;
  letter-spacing: 6rpx;
}
.list{
  width: 100%;
  height: 100rpx;
  font-size: 30rpx;
  border-bottom: 3rpx solid #d6d6d6;
}
.list .left{
  color: #333333;
  padding-left: 25rpx;
}
.list .right{
  color: #666666;
  padding-right: 25rpx;
}