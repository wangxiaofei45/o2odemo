.header{
  width: 100%;
  height: 240rpx;
  background-color: #ffffff;
  margin-bottom: 14rpx;
  text-align: center;
  font-size: 32rpx;
  color: #333;
  padding-top: 20rpx;
}
.header .input{
  width: 700rpx;
  height: 60rpx;
  border: 1rpx solid #d6d6d6;
  border-radius: 30rpx;
  margin: 0 auto;
  margin-bottom: 30rpx;
  position: relative;
  color: #999999;
  font-size: 24rpx;
}
.input .i1{
   width: 320rpx;
   text-align: right;
   padding-right: 25rpx;
}
.input .i2{
   width: 210rpx;
   text-align: left;
   padding-left: 25rpx;
}
.input .t{
   width: 66rpx;
   height: 60rpx;
   color: #666666;
   line-height: 60rpx;
   border-left: 1rpx solid #d6d6d6;
   border-right: 1rpx solid #d6d6d6;
}
.input image{
  width: 44rpx;
  height: 44rpx;
}
.header .w{
  line-height: 60rpx;
  color: #666666;
}
.header .w .red{
  color: #ff4965;
}
.list{
   width: 700rpx;
   height: 154rpx;
   margin: 0 auto;
   margin-bottom: 14rpx;
   position: relative;
}
.list .l{
   width: 106rpx;
   height: 106rpx;
   position: absolute;
   left: 0rpx;
   top: 24rpx;
   background-color: #f5f5f5;
   border-radius: 50%;
   z-index: 1;
   text-align: center;
}
.list .l image{
  width: 82rpx;
  height: 82rpx;
  margin-top: 12rpx;
  border-radius: 50%;
}
.list .r{
  width: 650rpx;
  height: 154rpx;
  background-color: #fff;
  position: absolute;
  left: 50rpx;
  top: 0rpx;
}
.list .r .r_con{
  width: 565rpx;
  margin-left: 60rpx;
  height: 154rpx;
  padding-left: 25rpx;
  position: relative;
}
.r .r_con .n{
  font-size: 30rpx;
  color: #333;
  padding-top: 30rpx;
  margin-bottom: 20rpx;
}
.r .r_con .t{
  font-size: 28rpx;
  color: #999999;
}
.r .r_con image{
  position: absolute;
  right: 25rpx;
  top: 25rpx;
  width: 44rpx;
  height: 44rpx;
}
.r .r_con .pull{
  font-size: 26rpx;
  position: absolute;
  right: 25rpx;
  bottom: 25rpx;
}
.r .r_con .pull .number{
  font-size: 28rpx;
  color: #ff4965;
  font-weight: 700;
}
.model{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0rpx;
  top: 0rpx;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 10
}
.model_con{
  width: 100%;
  height: 100%;
}
.picker{
  z-index: 20;
  width: 600rpx; 
  height: 450rpx;
  border-radius: 20rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -300rpx;
  margin-top: -150rpx;
  background-color: #fff;
  text-align:center;
}