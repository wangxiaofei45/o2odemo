page{
  background-color: #ffffff;
}
.containers{
  position: relative;
  width: 100%;
  height: 140rpx;
}
.select_pos_abs{
  z-index: 100;
}
.select_type{
  width: 750rpx;
  height: 110rpx;
  border-bottom: 1rpx solid #d6d6d6;
}
.select_type .select_con{
  width: 700rpx;
  height: 70rpx;
  line-height: 70rpx;
  border: 1rpx solid #d6d6d6;
  margin: 0 auto;
  border-radius: 10rpx;
  font-size: 34rpx;
}
.select_type .select_con .list{
  width: 350rpx;
  height: 70rpx;
  text-align: center;
}
.select_type .select_con .active{
  color: #ffffff;
  background-color: #FF4965;
}
.containers .list{
  width: 100%;
  height: 140rpx;
  border-bottom: 1rpx solid #d6d6d6;
  z-index: -2;  
}
.containers .list .left{
  padding-left: 25rpx;
}
.containers .list .left image{
  width: 92rpx;
  height: 92rpx;
  border-radius: 50%;
  margin-right: 25rpx;
  z-index: -2
}

.containers .list .left{
  font-size: 30rpx;
  color: #333333;
  font-weight: 600;
}
.containers .list .left .star{
  width: 44rpx;
  height: 44rpx;
  margin-left: 25rpx;
}
.containers .list .right image{
  width: 44rpx;
  height: 44rpx;
  margin-right: 25rpx;
}
.containers .list_pos{
  width: 100%;
  height: 140rpx;
  z-index: 10;
   position: absolute; 
  left: 0rpx;
  top: 0rpx;
  background-color: rgba(251, 155, 169,0.8);  
}
.containers .list_pos .pos_con{
  width: 100%;
  height: 140rpx;
}
.containers .list_pos .pos_con .list {
  text-align: center;
  color: #ffffff;
  font-size: 28rpx;
  width: 120rpx;
  height: 140rpx;
  margin-left: 10rpx;
  margin-right: 25rpx;
  border: none;
}
.containers .list_pos .pos_con .list image{
  width: 44rpx;
  height: 44rpx;
  margin-top: 19rpx;
}

