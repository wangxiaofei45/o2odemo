.bar_con{
  width: 100%;
  background-color: #ffffff;
  margin-top: 22rpx;
}
.bar_con .list{
  width: 50%;
  height: 200rpx;
  text-align: center;
  box-sizing: border-box;
}
.bar_con .list1{
  border-bottom: 3rpx solid #d6d6d6;
  border-right: 3rpx solid #d6d6d6;
}
.bar_con .list2{
  border-bottom: 3rpx solid #d6d6d6;
}
.bar_con .list3{
  border-right: 3rpx solid #d6d6d6;
}
.bar_con .list .name{
  font-size: 32rpx;
  font-weight: 600;
  color: #333333;
}
.bar_con .list image{
  width: 44rpx;
  height: 44rpx;
  margin-top: 45rpx;
  margin-bottom: 30rpx;
}