.header{
  width: 100%;
  height: 300rpx;
  color: #ffffff;
  position: relative;
  background-color: #fc7e92;
}
.header .header_list{
  font-size: 45rpx;
  font-weight: 700;
  padding-top: 70rpx;
}
.header .header_list .left{
  font-size: 34rpx;
  font-weight: 700;
  padding-left: 110rpx;
  padding-right: 45rpx;

}
.header .right_img{
  position: absolute;
  right: 0rpx;
  top: 43rpx;
  width: 200rpx;
  height: 214rpx;
}
.integral{
  width: 100%;
  height: 200rpx;
  background-color: #ffffff;
}
.integral .list{
  width: 350rpx;
  height: 100rpx;
  text-align: center;
}
.integral .list .name{
  font-size: 24rpx; color: #666666;
  font-weight: 700;
  padding-bottom: 35rpx;
}
.integral .list .number{
  font-size: 29rpx;
  color: #fc7e92;
  font-weight: 700;
}
