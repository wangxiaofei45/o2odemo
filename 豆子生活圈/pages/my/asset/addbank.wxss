.list{
  width: 100%;
  height: 90rpx;
  background-color: #fff;
  border-bottom: 3rpx solid #d6d6d6;
}
.list .left{
  width: 280rpx;
  height: 90rpx;
  margin-left: 25rpx;
  color: #666666;
  font-size: 30rpx;
}
.list .left image{
  width: 24rpx;
  height: 24rpx;
}
.list input{
  width: 355rpx;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 30rpx;
}
.list .right{
  width: 44rpx;
  height: 44rpx;
}
.sure{
  width: 700rpx;
  height: 90rpx;
  line-height: 90rpx;
  color: #ffffff;
  background-color: #ff4965;
  position: fixed;
  left: 25rpx;
  bottom: 50rpx;
}
.active{
  background-color: #999999;
}