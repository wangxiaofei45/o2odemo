.header{
  background-color: #ff4965;
  width: 100%;
  color: #ffffff;
  padding-top: 80rpx;
  box-sizing: border-box;
}
.header .header_title{
  font-size: 36rpx;
  text-align: center;
  padding-top: 75rpx;
  margin-bottom: 60rpx;
  font-weight: 700;
}
.header .header_number{
  font-size: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-weight: 700;
  text-align: center;
  padding-bottom: 70rpx;
}
.list_cons{
  width: 100%;
  height: 80rpx;
  padding-bottom: 50rpx;
}
.list_cons .lists{
  width: 50%;
  text-align: center;
}
.list_cons .lists .name{
  font-size: 32rpx;
  padding-bottom: 20rpx;
}
.list_cons .lists .number{
  font-size: 26rpx;
}
.list_cons .lists:nth-child(1){
  border-right: 4rpx solid #ffffff;
}
.list{
  width: 100%;
  height: 110rpx;
  background-color: #ffffff;
  margin-top: 22rpx;
  color: #666666;
  font-size: 30rpx;
}
.list image{
  width: 44rpx;
  height: 44rpx;
  margin-left: 25rpx;
  margin-right: 25rpx;
}