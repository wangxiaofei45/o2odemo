.header_bar{
  width: 100%;
  height: 225rpx;
  background-color: #fff;
  border-bottom: 3rpx solid #d6d6d6;
  margin-bottom: 22rpx;
}
.header_bar .bar_list{
  width: 180rpx;
  text-align: center;
  border-right: 3rpx solid #d6d6d6;
}
.header_bar .bar_list:nth-child(4){
  border: none;
}
.header_bar image{
  width: 70rpx;
  height: 70rpx;
}
.header_bar .number{
  font-size: 30rpx;
  color: #ff4965;
}
.list_con{
  width: 100%;
  height: 157rpx;
  border-top: 3rpx solid #d6d6d6;
  background-color: #fff;
}
.list_con .list{
  width: 315rpx;
  padding-left: 60rpx;
}
.list_con .list .title{
  color: #333333;
  font-size: 32rpx;
  margin-bottom: 30rpx;
}
.list_con .list .text{
  color: #999;
  font-size: 26rpx;
}
.list_con .list1{
  border-right: 3rpx solid #d6d6d6;
}