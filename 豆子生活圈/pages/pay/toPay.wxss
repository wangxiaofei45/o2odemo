page{
  background-color: #f5f5f5;
}
.title{
  height: 90rpx;
  background-color: #ffffff;
}
.title .left{
  margin-left: 25rpx;
  color: #333333;
  font-size: 30rpx;
}
.title .right{
  color: #ed5564;
  font-size: 28rpx;
  margin-right: 25rpx;
}
.title .right image{
   width: 24rpx;
   height: 24rpx;
}
.select{
  width: 100%;
  height: 82rpx;
  line-height: 82rpx;
  padding-left: 25rpx;
  color: #666666;
  font-size: 30rpx;
  letter-spacing: 8rpx;
}
/*复选框*/
.lists {
  /* display: block; */
  width: 100%;
  height: 140rpx;
  border-bottom: 3rpx solid #d6d6d6;
  background-color: #ffffff;
}
.lists .left{
  margin-left: 25px;
}
.lists .right{
  margin-right: 25px;
}
.lists .left image{
  width: 80rpx;
  height: 80rpx;
  margin-right: 15rpx;
}
.sure{
  width: 700rpx;
  height: 90rpx;
  color: #ffffff;
  background-color: #ff4965;
  position: fixed;
  left: 25rpx;
  bottom: 50rpx;
}