@import "store.wxss";
.hint{
   width: 100%;
   height: 116rpx;
   color: #666666;
   font-size: 30rpx;
   letter-spacing: 7rpx;
}
.hint image{
  width: 42rpx;
  height: 42rpx;
  margin-right: 20rpx;
}
.bottom{
  width: 300rpx;
  height: 40rpx;
  position: fixed;
  left: 50%;
  margin-left: -150rpx;
  bottom: 50rpx;
  text-align: center;
  font-size: 24rpx;
  color: #ff4965;
  letter-spacing: 5rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.bottom:active{
  text-shadow: 3rpx 3rpx 3rpx #ff4965;
}
.bottom image{
  width: 40rpx;
  height: 40rpx;
}