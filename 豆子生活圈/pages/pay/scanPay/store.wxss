page{
  background-color: #ffffff;
}
.title{
  width: 100%;
  margin-bottom: 70rpx;
}
.title image{
  width: 86rpx;
  height: 86rpx;
  border-radius: 10rpx;
  display: block;
  margin: 0 auto;
  margin-top: 90rpx; 
  }
  .title .name{
    width: 100%;
    color: #393a3f;
    font-size: 30rpx;
    font-weight: 700;
    margin: 0 auto;
    margin-top: 40rpx;
    text-align: center;
  }
.up{
  width: 564rpx;
  margin: 0 auto;
}
.up .title{
  width: 100%;
  
  font-size: 28rpx;
  color: #999999;
  font-weight: 700;
  margin-bottom: 50rpx;
  text-align: center;
  letter-spacing: 6rpx;
}
.up .hidden .titles{
  height: 120rpx;
}
.up .hidden .titles .left{
  color: #393a3f;
  font-size: 28rpx;
}
.up .hidden .titles .right{
  color: #ff4965;
  font-size: 24rpx;
}
.up .money{
  width: 100%;
  height: 130rpx;  
  font-size: 80rpx;
  /*暂时使用base64之后换网上图片*/
  background-image: url(data:image/png;base64,);
  
}
.up .money image{
  width: 80rpx;
  height: 80rpx;
}
.up button{
  background-color: #ff4965;
  color: #ffffff;
  font-weight: 700;
  letter-spacing: 5rpx;
  margin-top: 43rpx;
}
.collec_code_list{
  width: 565rpx;
  height: 120rpx;
  margin: 0 auto;
  border-bottom: 3rpx solid #e0e0e0;
  position: relative;
  color: #999999;
  font-size: 30rpx;
}
.up button:active{
  background-color: #393a3f;
}
